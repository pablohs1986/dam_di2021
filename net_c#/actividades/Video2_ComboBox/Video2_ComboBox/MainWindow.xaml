<Window x:Class="Video2_ComboBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Video2_ComboBox"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="cbPersonas" HorizontalAlignment="Left" Margin="193,64,0,0" VerticalAlignment="Top" Width="412" Height="43" SelectionChanged="cbPersonas_SelectionChanged"/>
        <Rectangle x:Name="rectangleResultado" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="100" Margin="193,189,0,0" Stroke="Black" VerticalAlignment="Top" Width="412"/>
        <Label x:Name="labelNombre" Content="Nombre" HorizontalAlignment="Left" Margin="295,228,0,0" VerticalAlignment="Top" Width="60"/>
        <Label x:Name="labelApellidos" Content="Apellidos" HorizontalAlignment="Left" Margin="441,228,0,0" VerticalAlignment="Top" Width="85"/>

    </Grid>
</Window>
